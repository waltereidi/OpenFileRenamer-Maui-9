@using ApplicationService.DAO
@using ApplicationService.Interfaces
@using FileManager.DAO


<table class="table table-primary table-striped w-100">
    <thead class="">
        <tr>
            <th >Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (TablePreviewDAO.TableRows row in DataTableFiles != null ? DataTableFiles.GetRows() : new())
        {
            <tr>
                <th class="table-division">@row.FileName</th>
            </tr>
        }
    </tbody>
    
</table>
@code{
    [Parameter]
    public ITablePreview DataTableFiles { get; set; }


}